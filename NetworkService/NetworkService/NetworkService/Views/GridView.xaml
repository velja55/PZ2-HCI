             d:DesignHeight="400" d:DesignWidth="800">
    <UserControl.DataContext>
        <viewModel:GridViewModel/>
    </UserControl.DataContext>

    
    
    
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.7*"/>
            <ColumnDefinition Width="0.5*"/>
            <ColumnDefinition Width="0.5*"/>
            <ColumnDefinition Width="0.5*"/>
            <ColumnDefinition Width="0.5*"/>
          
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            
        </Grid.RowDefinitions>


        <Grid Grid.RowSpan="3">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="8*"></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock Background="White" Foreground="{StaticResource DarkBlueBrush}" FontSize="20" FontWeight="Bold">All entities </TextBlock>
            <TreeView x:Name="TreeView" ItemsSource="{Binding EntitiesByTypes}" AllowDrop="False" Grid.Row="1" Cursor="Hand">
            <i:Interaction.Triggers>
                <i:EventTrigger EventName="SelectedItemChanged">
                    <i:InvokeCommandAction Command="{Binding SelectionChanged}"
                                   CommandParameter="{Binding SelectedItem, ElementName=TreeView}" />
                </i:EventTrigger>
                <i:EventTrigger EventName="MouseLeftButtonUp">
                    <i:InvokeCommandAction Command="{Binding MouseLeftButtonUp}"/>
                </i:EventTrigger>
            </i:Interaction.Triggers>
            
            <TreeView.Resources>
                <HierarchicalDataTemplate DataType="{x:Type model:EntitiesByType}" ItemsSource="{Binding Pressures}">
                    <StackPanel>
                        <TextBlock Text="{Binding Type}"></TextBlock>
                    </StackPanel>
                </HierarchicalDataTemplate>
                <DataTemplate DataType="{x:Type model:PressureInVentil}">
                   
                    <StackPanel Orientation="Horizontal">
                        <StackPanel>
                            <TextBlock Text="{Binding Id}" FontSize="12" FontWeight="SemiBold"></TextBlock>
                            <TextBlock Text="{Binding Name}" FontSize="12" FontWeight="SemiBold"></TextBlock>
                        </StackPanel>
                        <Image Source="{Binding Image}" Width="50" Height="50"></Image>

                    </StackPanel>
                    
                </DataTemplate>
                
            </TreeView.Resources>

        </TreeView>
        </Grid>
        
        
        <Border  BorderBrush="{Binding BorderBrushes[0]}" Background="Red" BorderThickness="2" Margin="10" Grid.Column="1">

            <Grid Background="{Binding GridBackgrounds[0]}">
            <Grid.RowDefinitions>
                <RowDefinition Height="0.52*"/>
                <RowDefinition Height="3*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
           

                <StackPanel Background="Gray" VerticalAlignment="Top" HorizontalAlignment="Right" Grid.Row="0" >
                    <Button Height="16" Name="Delete0" Command="{Binding Delete}" Cursor="Hand" CommandParameter="0">
                        <fa5:FontAwesome Icon="Regular_TimesCircle" FontSize="10"></fa5:FontAwesome>
                    </Button>
                </StackPanel>
            <ContentControl Content="{Binding Path=CollectionCanvas[0]}"  Grid.Row="1">
                <i:Interaction.Triggers>

                    <i:EventTrigger EventName="Drop">
                        <i:InvokeCommandAction Command="{Binding Drop}" CommandParameter="0"/>
                    </i:EventTrigger>
                        <i:EventTrigger EventName="MouseLeftButtonDown">
                            <i:InvokeCommandAction Command="{Binding MouseLeftButtonDownCanvas}" CommandParameter="0"/>

                        </i:EventTrigger>

                        <i:EventTrigger EventName="MouseLeftButtonUp">
                            <i:InvokeCommandAction Command="{Binding MouseLeftButtonUp}"/>

                        </i:EventTrigger>

                    </i:Interaction.Triggers>
                   


            </ContentControl>
                <Grid Grid.Row="2">
