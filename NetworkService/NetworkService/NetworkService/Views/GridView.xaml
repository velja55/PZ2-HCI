                        <ColumnDefinition Width="*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <TextBlock Text="ID:" FontSize="12" Margin="5,0,0,0"/>
                    <TextBlock  Name="IdTextBlock1" Text="{Binding SelectedId[0]}" Margin="5,0,0,0" Grid.Row="1" FontSize="12"></TextBlock>
                    <TextBlock Text="Value:" FontSize="12" Grid.Column="1"/>
                    <TextBlock  Name="ValueTextBlock1" Text="{Binding SelectedValue[0]}" Grid.Column="1" Grid.Row="1" FontSize="12"></TextBlock>
                    
                </Grid>


            </Grid>
        </Border>


